{
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 100,
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "jsxBracketSameLine": false,
    "plugins": ["prettier-plugin-tailwindcss"]
  }
  